{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "incremental": false,
    "isolatedModules": true,
    "lib": [
      "es2022",
      "DOM.Iterable"
    ],
    "module": "Node16",
    "moduleResolution": "Node16",
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2023",
    "outDir": "dist"
  }
}